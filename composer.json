{
  "name": "securerun/bubblewrap-sandbox",
  "description": "Security layer to run external commands inside bubblewrap for Laravel 5 through 12.",
  "keywords": ["laravel", "bubblewrap", "sandbox", "security", "bwrap"],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "illuminate/support": "5.*|6.*|7.*|8.*|9.*|10.*|11.*|12.*",
    "symfony/process": "^3.4|^4.4|^5.4|^6.4|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5 || ^7.5 || ^8.5 || ^9.6 || ^10.5 || ^11.0"
  },
  "autoload": {
    "psr-4": {
      "SecureRun\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SecureRun\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SecureRun\\Sandbox\\BubblewrapServiceProvider"
      ],
      "aliases": {
        "BubblewrapSandbox": "SecureRun\\BubblewrapSandbox"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
